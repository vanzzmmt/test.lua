

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({
    Name = "Dive Down Grok Hub",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "DiveDownGrok"
})

local FarmTab = Window:MakeTab({Name = "Farm", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local PlayerTab = Window:MakeTab({Name = "Player", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local TeleTab = Window:MakeTab({Name = "Teleport", Icon = "rbxassetid://4483345998", PremiumOnly = false})
local MiscTab = Window:MakeTab({Name = "Misc", Icon = "rbxassetid://4483345998", PremiumOnly = false})

getgenv().InfO2 = false
getgenv().AutoCatch = false
getgenv().ESPRare = false
getgenv().Fly = false
getgenv().Highlights = {}

local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local root = char:WaitForChild("HumanoidRootPart")
local hum = char:WaitForChild("Humanoid")

local function getRareFish()
    local fish = {}
    local rarities = {"Divine", "Secret", "Mythic"}
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("Model") and obj.PrimaryPart and obj:FindFirstChild("Rarity") then
            local rarity = obj.Rarity.Value or ""
            for _, r in ipairs(rarities) do
                if rarity:find(r) then
                    table.insert(fish, obj)
                    break
                end
            end
        end
    end
    return fish
end

local function tpToFish(fish)
    if fish.PrimaryPart then
        root.CFrame = fish.PrimaryPart.CFrame * CFrame.new(0, 5, -3)
        wait(0.1)
        local prompt = fish:FindFirstChildOfClass("ProximityPrompt")
        if prompt then fireproximityprompt(prompt) end
    end
end

-- Farm Tab
FarmTab:AddToggle({
    Name = "Infinite Oxygen",
    Default = false,
    Callback = function(Value)
        getgenv().InfO2 = Value
        OrionLib:MakeNotification({Name = "O2", Content = Value and "ON - No Drown!" or "OFF", Image = "rbxassetid://4483345998", Time = 3})
    end    
})

FarmTab:AddToggle({
    Name = "Auto Catch Rare Fish",
    Default = false,
    Callback = function(Value)
        getgenv().AutoCatch = Value
        OrionLib:MakeNotification({Name = "Auto Farm", Content = Value and "Hunting Divine/Secret/Mythic..." or "Stopped", Image = "rbxassetid://4483345998", Time = 3})
    end    
})

FarmTab:AddToggle({
    Name = "Rare Fish ESP",
    Default = false,
    Callback = function(Value)
        getgenv().ESPRare = Value
        if Value then
            for _, hl in pairs(getgenv().Highlights) do hl:Destroy() end
            getgenv().Highlights = {}
            for _, fish in pairs(getRareFish()) do
                local hl = Instance.new("Highlight")
                hl.FillColor = Color3.fromRGB(255, 215, 0)
                hl.OutlineColor = Color3.fromRGB(255, 0, 0)
                hl.FillTransparency = 0.4
                hl.OutlineTransparency = 0
                hl.Parent = fish
                table.insert(getgenv().Highlights, hl)
            end
        else
            for _, hl in pairs(getgenv().Highlights) do hl:Destroy() end
            getgenv().Highlights = {}
        end
    end    
})

-- Player Tab
PlayerTab:AddSlider({
    Name = "Walk Speed",
    Min = 16,
    Max = 200,
    Default = 16,
    Color = Color3.fromRGB(255,255,255),
    Increment = 1,
    ValueName = "Speed",
    Callback = function(Value)
        hum.WalkSpeed = Value
    end    
})

PlayerTab:AddSlider({
    Name = "Jump Power",
    Min = 50,
    Max = 200,
    Default = 50,
    Color = Color3.fromRGB(255,255,255),
    Increment = 1,
    ValueName = "Power",
    Callback = function(Value)
        hum.JumpPower = Value
    end    
})

PlayerTab:AddToggle({
    Name = "Fly (Basic)",
