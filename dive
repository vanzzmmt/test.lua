    Callback = function(Value)
        getgenv().Fly = Value
    end    
})

-- Teleport Tab
TeleTab:AddButton({
    Name = "Teleport to Surface",
    Callback = function()
        root.CFrame = CFrame.new(0, 50, 0)
    end    
})

TeleTab:AddButton({
    Name = "Teleport to Mid Zone",
    Callback = function()
        root.CFrame = CFrame.new(0, -1000, 0)
    end    
})

TeleTab:AddButton({
    Name = "Teleport to Deep/Last Zone",
    Callback = function()
        root.CFrame = CFrame.new(0, -3000, 0)
        OrionLib:MakeNotification({Name = "TP", Content = "Deep Zone Reached!", Time = 3})
    end    
})

TeleTab:AddButton({
    Name = "Auto Sell Inventory",
    Callback = function()
        pcall(function()
            game.ReplicatedStorage.Remotes.SellAll:FireServer()  -- adjust nama remote kalau beda
        end)
    end    
})

-- Loop utama
game:GetService("RunService").RenderStepped:Connect(function()
    if getgenv().InfO2 then
        pcall(function()
            player.leaderstats.Oxygen.Value = math.huge  -- adjust kalau nama beda (Oxygen/O2)
        end)
        hum.Health = math.huge
    end

    if getgenv().AutoCatch then
        local closest = nil
        local minDist = math.huge
        for _, fish in pairs(getRareFish()) do
            local dist = (root.Position - fish.PrimaryPart.Position).Magnitude
            if dist < minDist and dist < 600 then
                minDist = dist
                closest = fish
            end
        end
        if closest then
            tpToFish(closest)
        end
    end

    if getgenv().Fly then
        local cam = workspace.CurrentCamera
        local direction = cam.CFrame.LookVector * 50
        root.Velocity = direction
    end
end)

OrionLib:Init()
print("Dive Down Orion Hub Loaded! UI harus muncul sekarang.")
