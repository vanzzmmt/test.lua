local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("ğŸ£ Go Fishing Kavo Hub | by Grok", "DarkTheme")

local MainTab = Window.NewTab("ğŸ£ Auto Farm")
local PlayerTab = Window.NewTab("ğŸ‘¤ Player")
local TeleportTab = Window.NewTab("ğŸ“ Teleport")

-- Services & Remotes (FIXED 2026)
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local events = ReplicatedStorage:WaitForChild("events")
local fishing = events:WaitForChild("fishing")

local castRod = fishing:WaitForChild("castRod") or fishing:WaitForChild("CastRod")
local fightClick = fishing:WaitForChild("fightClick")
local sellRemote = events:WaitForChild("sell") or ReplicatedStorage:FindFirstChild("Remotes") and ReplicatedStorage.Remotes:FindFirstChild("SellAll")

-- Variables
local AutoFish = false
local AutoCast = false
local AutoSell = false
local WalkSpeed = 16
local JumpPower = 50

-- FARM TAB
local FarmSection = MainTab.NewSection("ğŸš€ Instant Farm")

MainTab.NewToggle("ğŸ£ Auto Cast Rod", "Auto cast untuk mulai fishing", function(state)
    AutoCast = state
    if state then
        spawn(function()
            while AutoCast do
                pcall(function()
                    castRod:FireServer("Primary")
                end)
                task.wait(1)  -- Cast every 1s
            end
        end)
    end
end)

MainTab.NewToggle("âš”ï¸ Auto FightClick (INSTANT Catch!)", "Spam fight = fish god mode", function(state)
    AutoFish = state
    if state then
        spawn(function()
            while AutoFish do
                pcall(function()
                    fightClick:FireServer()
                end)
                task.wait(0.01)  -- Ultra fast!
            end
        end)
    end
end)

MainTab.NewToggle("ğŸ’° Auto Sell (Every 10s)", "Auto jual ikan", function(state)
    AutoSell = state
    if state then
        spawn(function()
            while AutoSell do
                pcall(function()
                    if sellRemote then
                        sellRemote:FireServer("All")
                    end
                end)
                task.wait(10)
            end
        end)
    end
end)

MainTab.NewButton("ğŸ’¸ Sell All NOW", "Jual manual", function()
    pcall(function()
        if sellRemote then sellRemote:FireServer("All") end
    end)
    Library:Notify("ğŸ¤‘ Sell Fired!", 3)
end)

-- PLAYER TAB
local PlayerSection = PlayerTab.NewSection("Speed & Jump")

PlayerTab.NewSlider("Walk Speed", "16-500", function(s)
    WalkSpeed = s
end)

PlayerTab.NewSlider("Jump Power", "50-500", function(j)
    JumpPower = j
end)

-- Apply player mods
spawn(function()
    while task.wait() do
        if player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.WalkSpeed = WalkSpeed
            player.Character.Humanoid.JumpPower = JumpPower
        end
    end
end)

-- TELEPORT TAB
local TPSection = TeleportTab.NewSection("ğŸ“ Spots")

TeleportTab.NewButton("ğŸï¸ Spawn", "", function()
    if player.Character then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 10, 0)
    end
end)

TeleportTab.NewButton("ğŸ£ Spot 1 (Rare Fish)", "", function()
    if player.Character then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(-150, 5, -200)
    end
end)

TeleportTab.NewButton("ğŸ£ Spot 2", "", function()
    if player.Character then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(100, 5, 50)
    end
end)

Library:Notify("ğŸ£ Kavo Hub LOADED di Delta! ON Auto Cast + FightClick = INSTANT $$$ ğŸ”¥", 5)

print("ğŸš€ Go Fishing Kavo by Grok - Press RightShift buat toggle GUI!")PlayerTab:CreateSlider({
   Name = "Walk Speed",
   Range = {16, 300},
   Increment = 1,
   CurrentValue = 16,
   Flag = "SpeedSlider",
   Callback = function(Value)
      getgenv().WalkSpeed = Value
   end,
})

PlayerTab:CreateSlider({
   Name = "Jump Power",
   Range = {50, 300},
   Increment = 1,
   CurrentValue = 50,
   Flag = "JumpSlider",
   Callback = function(Value)
      getgenv().JumpPower = Value
   end,
})

PlayerTab:CreateToggle({
   Name = "ğŸ¦˜ Infinite Jump",
   CurrentValue = false,
   Flag = "InfJumpV2",
   Callback = function(Value)
      getgenv().InfJumpEnabled = Value
   end,
})

-- Apply Mods
local function applyMods()
   if player.Character and player.Character:FindFirstChild("Humanoid") then
      player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeed
      player.Character.Humanoid.JumpPower = getgenv().JumpPower
   end
end

player.CharacterAdded:Connect(applyMods)
RunService.Heartbeat:Connect(applyMods)

game:GetService("UserInputService").JumpRequest:Connect(function()
   if getgenv().InfJumpEnabled then
      if player.Character then
         player.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
      end
   end
end)

