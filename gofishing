local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "üé£ Go Fishing Hub v2 | Grok Fixed",
   LoadingTitle = "Loading... (Press F1 for GUI)",
   LoadingSubtitle = "Instant Fish ON! üî•",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "GoFishingGrokV2", 
      FileName = "vanzzConfig"
   },
   Discord = { Enabled = false },
   KeySystem = false
})

local MainTab = Window:CreateTab("üé£ Auto Farm", 4483362458)
local PlayerTab = Window:CreateTab("üë§ Player", 4483362458)
local TeleportTab = Window:CreateTab("üìç Teleport", 4483362458)

-- Services & Correct Remotes (FIXED!)
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
local events = ReplicatedStorage:WaitForChild("events", 10)
local fishing = events:WaitForChild("fishing", 10)
local fightClick = fishing:WaitForChild("fightClick", 10)

if not fightClick then 
   Rayfield:Notify({Title = "‚ùå Error", Content = "Remotes not found! Rejoin server.", Duration = 5})
   return 
end

-- Variables
getgenv().AutoFishEnabled = false
getgenv().AutoSellEnabled = false
getgenv().WalkSpeed = 16
getgenv().JumpPower = 50
getgenv().InfJumpEnabled = false

-- MAIN TAB
local MainSection = MainTab:CreateSection("üöÄ Instant Auto Farm")

MainTab:CreateToggle({
   Name = "üé£ Auto FightClick (INSTANT Fish God Mode)",
   CurrentValue = false,
   Flag = "AutoFishV2",
   Callback = function(Value)
      getgenv().AutoFishEnabled = Value
      if Value then
         spawn(function()
            while getgenv().AutoFishEnabled do
               pcall(function()
                  fightClick:FireServer()
               end)
               task.wait(0.01)  -- Super fast = NONSTOP fish!
            end
         end)
         Rayfield:Notify({Title = "üöÄ ON", Content = "Instant Fishing ACTIVE! üí∞", Duration = 3})
      else
         Rayfield:Notify({Title = "‚èπÔ∏è OFF", Content = "Auto Fish STOPPED.", Duration = 2})
      end
   end,
})

MainTab:CreateToggle({
   Name = "üí∞ Auto Sell (Every 8s)",
   CurrentValue = false,
   Flag = "AutoSellV2",
   Callback = function(Value)
      getgenv().AutoSellEnabled = Value
      if Value then
         spawn(function()
            while getgenv().AutoSellEnabled do
               pcall(function()
                  -- Try common sell remotes (works in most servers)
                  local remotes = ReplicatedStorage:FindFirstChild("Remotes")
                  if remotes then
                     local sellAll = remotes:FindFirstChild("SellAll") or remotes:FindFirstChild("SellFish") or remotes:FindFirstChild("SellAllFish")
                     if sellAll then sellAll:FireServer() end
                  end
                  -- Alt paths
                  local sellEvent = events:FindFirstChild("sell")
                  if sellEvent then sellEvent:FireServer("All") end
               end)
               task.wait(8)
            end
         end)
      end
   end,
})

MainTab:CreateButton({
   Name = "üí∏ SELL ALL NOW (Multi-Try)",
   Callback = function()
      pcall(function()
         local remotes = ReplicatedStorage:FindFirstChild("Remotes")
         if remotes then
            (remotes:FindFirstChild("SellAll") or remotes:FindFirstChild("SellFish") or remotes:FindFirstChild("SellAllFish")):FireServer()
         end
         local sellEvent = events:FindFirstChild("sell")
         if sellEvent then sellEvent:FireServer("All") end
         print("ü§ë Tried Sell All!")
         Rayfield:Notify({Title = "üí∞", Content = "Sell All Fired!", Duration = 2})
      end)
   end,
})

-- PLAYER TAB
local PlayerSection = PlayerTab:CreateSection("Speed & Jump")

PlayerTab:CreateSlider({
   Name = "Walk Speed",
   Range = {16, 300},
   Increment = 1,
   CurrentValue = 16,
   Flag = "SpeedSlider",
   Callback = function(Value)
      getgenv().WalkSpeed = Value
   end,
})

PlayerTab:CreateSlider({
   Name = "Jump Power",
   Range = {50, 300},
   Increment = 1,
   CurrentValue = 50,
   Flag = "JumpSlider",
   Callback = function(Value)
      getgenv().JumpPower = Value
   end,
})

PlayerTab:CreateToggle({
   Name = "ü¶ò Infinite Jump",
   CurrentValue = false,
   Flag = "InfJumpV2",
   Callback = function(Value)
      getgenv().InfJumpEnabled = Value
   end,
})

-- Apply Mods
local function applyMods()
   if player.Character and player.Character:FindFirstChild("Humanoid") then
      player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeed
      player.Character.Humanoid.JumpPower = getgenv().JumpPower
   end
end

player.CharacterAdded:Connect(applyMods)
RunService.Heartbeat:Connect(applyMods)

game:GetService("UserInputService").JumpRequest:Connect(function()
   if getgenv().InfJumpEnabled then
      if player.Character then
         player.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
      end
   end
end)

-- TELEPORT (Best Spots 2026)
local TPSection = TeleportTab:CreateSection("üìç Islands & Spots")

TeleportTab:CreateButton({
   Name = "üèùÔ∏è Spawn",
   Callback = function()
      if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
         player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 10, 0)
      end
   end,
})

TeleportTab:CreateButton({
   Name = "üé£ Best Fish Spot 1 (Rare)",
   Callback = function()
      if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
         player.Character.HumanoidRootPart.CFrame = CFrame.new(-150, 5, -200)  -- Adjust if needed
      end
   end,
})

TeleportTab:CreateButton({
   Name = "üé£ Best Fish Spot 2",
   Callback = function()
      if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
         player.Character.HumanoidRootPart.CFrame = CFrame.new(100, 5, 50)
      end
   end,
})

-- Loaded!
Rayfield:Notify({
   Title = "üé£ HUB v2 LOADED! ‚úÖ",
   Content = "Equip Rod ‚Üí Toggle Auto Fish ‚Üí INSTANT $$$! Report if sell not work.",
   Duration = 6,
   Image = 4483362458,
})

print("üöÄ **Go Fishing FIXED Rayfield v2 by Grok! Spam FightClick = God Mode** üòà")UserInputService.JumpRequest:Connect(function()
   if getgenv().InfJumpEnabled and player.Character then
      player.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
   end
end)

-- Update Speed/Jump on Spawn
player.CharacterAdded:Connect(function(char)
   local humanoid = char:WaitForChild("Humanoid")
   humanoid.WalkSpeed = getgenv().WalkSpeed
   humanoid.JumpPower = getgenv().JumpPower
end)

-- Speed/Jump Loop
RunService.Heartbeat:Connect(function()
   if player.Character and player.Character:FindFirstChild("Humanoid") then
      player.Character.Humanoid.WalkSpeed = getgenv().WalkSpeed
      player.Character.Humanoid.JumpPower = getgenv().JumpPower
   end
end)

-- TELEPORT TAB (Common Spots - Adjust CFrame if needed)
local TPSection = TeleportTab:CreateSection("Teleports")

TeleportTab:CreateButton({
   Name = "üèùÔ∏è Spawn/Island",
   Callback = function()
      player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 10, 0)  -- Adjust to spawn
   end,
})

TeleportTab:CreateButton({
   Name = "üé£ Best Fishing Spot",
   Callback = function()
      player.Character.HumanoidRootPart.CFrame = CFrame.new(-50, 5, -50)  -- Example good spot
   end,
})

-- Notify Loaded
Rayfield:Notify({
   Title = "üé£ Go Fishing Hub Loaded!",
   Content = "Auto Fish ON ‚Üí INSTANT fish! Test di alt acc. Report bugs!",
   Duration = 5,
   Image = 4483362458,
})

print("üöÄ **Go Fishing Rayfield Hub by Grok LOADED! Press Home to open GUI** üòà")
